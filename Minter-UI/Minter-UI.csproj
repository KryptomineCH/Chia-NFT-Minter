<Project Sdk="Microsoft.NET.Sdk">
    
  <PropertyGroup>
	  <IncludeAllContentForSelfExtract>true</IncludeAllContentForSelfExtract>
      <OutputType>WinExe</OutputType>
      <TargetFramework>net7.0-windows</TargetFramework>
      <RootNamespace>Minter_UI</RootNamespace>
      <Nullable>enable</Nullable>
      <UseWPF>true</UseWPF>
      <ApplicationIcon>Resources\favicon.ico</ApplicationIcon>
      <Platforms>AnyCPU;x64</Platforms>
	  <Deterministic>false</Deterministic>
	  <AssemblyVersion>0.0.*</AssemblyVersion>
	  <FileVersion>0.0.*</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\Application_Icon.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Resources\favicon.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Chia-Client-API" Version="1.6.2.4" />
    <PackageReference Include="Microsoft.Web.WebView2" Version="1.0.1462.37" />
    <PackageReference Include="Multithreading_Library" Version="1.0.0" />
    <PackageReference Include="NFT.Storage.Net" Version="1.0.0.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Chia-NFT-Minter\Chia-NFT-Minter.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\Application_Icon.png" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
